<% include header %>

<h1><%= title %></h1>

<form class="form-inline">
	<div class="form-group">
		<label class="sr-only" for="exampleInputPassword0">Select date</label>
		<input type="text" class="form-control span0" id="exampleInputPassword0" value="" placeholder="Date" data-date-format="mm/dd/yy">
	</div>
	<button type="submit" class="btn btn-default">View calories by this Day!</button>
</form>

<div class="table-responsive">
	<table class="table user-list">
		<thead>
		<tr>
			<th><span>Product</span></th>
			<th><span>Calories Value</span></th>
			<th class="text-center"><span>Name</span></th>
			<th><span>Date</span></th>
		</tr>
		</thead>
		<tbody>
		<% for(var i = 0; i < calories.length; i++) {%>
		<% include calorie %>
		<% } %>
		</tbody>
	</table>
</div>

<script type="text/javascript" src="javascripts/profile.js"></script>

<% include footer %>